{
  "name": "pump-gym",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.js",
  "description": "This app is for controlling and monitoring your gym equipment.",
  "author": {
    "name": "Mahmoud Alaa Ibrahim Mostafa",
    "email": "mahmoud.alaa.dev1@gmail.com",
    "url": "https://www.linkedin.com/in/mahmoudalaa2/"
  },
  "scripts": {
    "seed": "npx prisma migrate reset && npx ts-node prisma/seed.ts",
    "dev": "npm-run-all -p dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "pnpm run transpile:electron && cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc -p src/electron/tsconfig.json",
    "dist:mac": "pnpm run transpile:electron && pnpm run build && electron-builder --mac --arm64",
    "dist:win": "pnpm run transpile:electron && pnpm run build && electron-builder --win --x64",
    "dist:linux": "pnpm run transpile:electron && pnpm run build && electron-builder --linux --x64"
  },
  "dependencies": {
